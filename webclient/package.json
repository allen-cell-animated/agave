{
  "name": "agave-webclient",
  "version": "1.0.0",
  "description": "client library for using agave as remote rendering server",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run transpileES && npm run build-types",
    "build-types": "tsc -p tsconfig.types.json",
    "clean": "rimraf es/",
    "format": "prettier --write src/**/*.js",
    "gh-build": "webpack --config webpack.dev.js",
    "dev": "webpack serve --config webpack.dev.js",
    "start": "webpack serve --config webpack.dev.js",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --ext .jsx --ext .js --ext .ts ./src",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.test.json' mocha --require ts-node/register src/**/test/*.js",
    "transpileES": "babel src --out-dir es --extensions .js,.ts --ignore **/*.test.js",
    "typeCheck": "tsc -p tsconfig.json --noEmit"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "acorn": "^8.8.2",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "html-webpack-plugin": "^5.5.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3",
    "webpack": "^5.85.1",
    "webpack-cli": "^5.1.3",
    "webpack-dev-server": "^4.15.0"
  }
}
